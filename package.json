{
  "name": "parcel-vanilla-starter",
  "private": true,
  "version": "0.0.0",
  "source": "src/index.html",
  "scripts": {
    "build:ejs": "ejs src/index.ejs -o src/index.html",
    "dev": "chokidar 'src/index.ejs' -c 'npm run build:ejs && touch src/index.html' & parcel src/index.html --open",
    "build": "npm run build:ejs && parcel build src/index.html"
  },
  "dependencies": {
    "bulma": "^1.0.4",
    "chess.js": "^1.2.0",
    "chessground": "^9.1.1",
    "open": "^10.1.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.14.4",
    "chokidar-cli": "^3.0.0",
    "ejs-cli": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "parcel": "latest",
    "svgo": "^3.3.2"
  }
}
